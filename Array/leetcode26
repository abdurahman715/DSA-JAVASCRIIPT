//âœ…Leetcode-26. Remove Duplicates from Sorted Array
function removeDuplicates(nums) {
  let x = 0;
  for (let i = 0; i < nums.length; i++) {
    if (nums[i] > nums[x]) {
      x = x + 1;
      nums[x] = nums[i];
    }
  }
  console.log(nums);
  return x + 1;
}
let arr = [0, 1, 1, 2, 2, 3, 3, 4];
let result = removeDuplicates(arr);
console.log(result);
