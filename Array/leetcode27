//âœ…Leetcode-27. Remove Element
function removeElement(nums, value) {
  let x = 0;
  for (let i = 0; i < nums.length; i++) {
    //Shift elements to left if it is not equal to value
    if (nums[i] !== value) {
      nums[x] = nums[i];
      x++;
    }
  }
  console.log(nums);
  return x;
}
let nums = [3, 1, 3, 4, 3, 3, 3, 7];
let val = 3;
let result = removeElement(nums, val);
console.log(result);
